{
  "name": "@ddgll/vue3-trend",
  "description": "Convert images from an url or an input file to another type",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddgll/vue3-trend.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-trend.umd.js",
  "module": "./dist/vue3-trend.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-trend.es.js",
      "require": "./dist/vue3-trend.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "vue",
    "vue3",
    "trend",
    "svg",
    "graph",
    "data-visualization",
    "component"
  ],
  "author": "David Grill <grilldotcom@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddgll/vue3-trend/issues"
  },
  "homepage": "https://github.com/ddgll/vue3-trend#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && mkdir dist/types && cp lib/index.d.ts dist/types/index.d.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vue-tsc": "^3.1.0"
  }
}
